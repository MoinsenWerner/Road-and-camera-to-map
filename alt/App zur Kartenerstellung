    viewModelScope.launch {
        </span><span><span class="hljs-keyword">try</span></span><span> {
            </span><span><span class="hljs-keyword">val</span></span><span> latMin = currentLat - </span><span><span class="hljs-number">0.005</span></span><span>
            </span><span><span class="hljs-keyword">val</span></span><span> latMax = currentLat + </span><span><span class="hljs-number">0.005</span></span><span>
            </span><span><span class="hljs-keyword">val</span></span><span> lonMin = currentLon - </span><span><span class="hljs-number">0.005</span></span><span>
            </span><span><span class="hljs-keyword">val</span></span><span> lonMax = currentLon + </span><span><span class="hljs-number">0.005</span></span><span>

            </span><span><span class="hljs-keyword">val</span></span><span> segments = ApiClient.api.downloadArea(latMin, latMax, lonMin, lonMax)

            </span><span><span class="hljs-keyword">val</span></span><span> dao = MapDatabase.</span><span><span class="hljs-keyword">get</span></span><span>(getApplication()).dao()

            </span><span><span class="hljs-keyword">for</span></span><span> (seg </span><span><span class="hljs-keyword">in</span></span><span> segments) {
                </span><span><span class="hljs-keyword">val</span></span><span> geoJson = kotlinx.serialization.json.Json.parseToJsonElement(seg.geometry)
                </span><span><span class="hljs-keyword">val</span></span><span> coords = geoJson.jsonObject[</span><span><span class="hljs-string">"coordinates"</span></span><span>]
                    ?.jsonArray?.firstOrNull()
                    ?.jsonArray
                    ?.map { point -&gt;
                        </span><span><span class="hljs-keyword">val</span></span><span> p = point.jsonArray
                        listOf(p[</span><span><span class="hljs-number">1</span></span><span>].toString().toDouble(), p[</span><span><span class="hljs-number">0</span></span><span>].toString().toDouble())
                    } ?: </span><span><span class="hljs-keyword">continue</span></span><span>

                </span><span><span class="hljs-keyword">val</span></span><span> id = coords.joinToString().hashCode().toString()
                dao.insertSegment(
                    SegmentEntity(
                        id = id,
                        coordinates = coords,
                        speedLimit = seg.speed_limit,
                        source = seg.source
                    )
                )
            }

            println(</span><span><span class="hljs-string">"Sync abgeschlossen: <span class="hljs-subst">${segments.size}</span></span></span><span> neue Segmente")
        } </span><span><span class="hljs-keyword">catch</span></span><span> (e: Exception) {
            e.printStackTrace()
        }
    }
}
