</span><span><span class="hljs-keyword">import</span></span><span> kotlinx.serialization.Serializable
</span><span><span class="hljs-keyword">import</span></span><span> retrofit2.http.*

</span><span><span class="hljs-keyword">interface</span></span><span> </span><span><span class="hljs-title class_">ApiService</span></span><span> {

    </span><span><span class="hljs-meta">@POST(<span class="hljs-string">"/upload_segment"</span></span></span><span>)
    </span><span><span class="hljs-keyword">suspend</span></span><span> </span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">uploadSegment</span></span><span><span class="hljs-params">(<span class="hljs-meta">@Body</span></span></span><span> segment: </span><span><span class="hljs-type">SegmentUploadRequest</span></span><span>): ApiResponse

    </span><span><span class="hljs-meta">@GET(<span class="hljs-string">"/download_area"</span></span></span><span>)
    </span><span><span class="hljs-keyword">suspend</span></span><span> </span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">downloadArea</span></span><span><span class="hljs-params">(
        </span></span><span><span class="hljs-meta">@Query(<span class="hljs-string">"lat_min"</span></span></span><span>) latMin: </span><span><span class="hljs-type">Double</span></span><span>,
        </span><span><span class="hljs-meta">@Query(<span class="hljs-string">"lat_max"</span></span></span><span>) latMax: </span><span><span class="hljs-type">Double</span></span><span>,
        </span><span><span class="hljs-meta">@Query(<span class="hljs-string">"lon_min"</span></span></span><span>) lonMin: </span><span><span class="hljs-type">Double</span></span><span>,
        </span><span><span class="hljs-meta">@Query(<span class="hljs-string">"lon_max"</span></span></span><span>) lonMax: </span><span><span class="hljs-type">Double</span></span><span>
    ): List&lt;SegmentResponse&gt;
}

</span><span><span class="hljs-meta">@Serializable</span></span><span>
