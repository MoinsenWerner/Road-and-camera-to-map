</span><span><span class="hljs-keyword">import</span></span><span> android.</span><span><span class="hljs-keyword">annotation</span></span><span>.SuppressLint
</span><span><span class="hljs-keyword">import</span></span><span> android.content.Context
</span><span><span class="hljs-keyword">import</span></span><span> android.location.Location
</span><span><span class="hljs-keyword">import</span></span><span> android.os.Looper
</span><span><span class="hljs-keyword">import</span></span><span> com.google.android.gms.location.*
</span><span><span class="hljs-keyword">import</span></span><span> kotlinx.coroutines.channels.awaitClose
</span><span><span class="hljs-keyword">import</span></span><span> kotlinx.coroutines.flow.callbackFlow
</span><span><span class="hljs-keyword">import</span></span><span> kotlinx.coroutines.flow.Flow

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">GpsTracker</span></span><span>(</span><span><span class="hljs-keyword">private</span></span><span> </span><span><span class="hljs-keyword">val</span></span><span> context: Context) {

    </span><span><span class="hljs-keyword">private</span></span><span> </span><span><span class="hljs-keyword">val</span></span><span> fusedLocationClient = LocationServices.getFusedLocationProviderClient(context)

    </span><span><span class="hljs-meta">@SuppressLint(<span class="hljs-string">"MissingPermission"</span></span></span><span>)
    </span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">observeSpeed</span></span><span><span class="hljs-params">()</span></span><span>: Flow&lt;</span><span><span class="hljs-built_in">Float</span></span><span>&gt; = callbackFlow {
        </span><span><span class="hljs-keyword">val</span></span><span> request = LocationRequest.Builder(
            Priority.PRIORITY_HIGH_ACCURACY, </span><span><span class="hljs-number">500L</span></span><span> </span><span><span class="hljs-comment">// alle 500 ms</span></span><span>
        ).apply {
            setMinUpdateDistanceMeters(</span><span><span class="hljs-number">0f</span></span><span>)
            setMinUpdateIntervalMillis(</span><span><span class="hljs-number">300L</span></span><span>)
        }.build()

        </span><span><span class="hljs-keyword">val</span></span><span> callback = </span><span><span class="hljs-keyword">object</span></span><span> : LocationCallback() {
            </span><span><span class="hljs-keyword">override</span></span><span> </span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">onLocationResult</span></span><span><span class="hljs-params">(result: <span class="hljs-type">LocationResult</span></span></span><span>) {
                </span><span><span class="hljs-keyword">val</span></span><span> location: Location? = result.lastLocation
                </span><span><span class="hljs-keyword">if</span></span><span> (location != </span><span><span class="hljs-literal">null</span></span><span>) {
                    </span><span><span class="hljs-keyword">val</span></span><span> speedKmh = location.speed * </span><span><span class="hljs-number">3.6f</span></span><span>
                    trySend(speedKmh)
                }
            }
        }

        fusedLocationClient.requestLocationUpdates(
            request,
            callback,
            Looper.getMainLooper()
        )

        awaitClose {
            fusedLocationClient.removeLocationUpdates(callback)
        }
    }
}
