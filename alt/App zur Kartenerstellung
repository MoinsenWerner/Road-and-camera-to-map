    </span><span><span class="hljs-keyword">val</span></span><span> detectionMode: Flow&lt;String&gt; = context.settingsDataStore.</span><span><span class="hljs-keyword">data</span></span><span>.map { it[detectionModeKey] ?: </span><span><span class="hljs-string">"camera"</span></span><span> }

    </span><span><span class="hljs-keyword">suspend</span></span><span> </span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">setSyncEnabled</span></span><span><span class="hljs-params">(value: <span class="hljs-type">Boolean</span></span></span><span>) =
        context.settingsDataStore.edit { it[syncKey] = value }

