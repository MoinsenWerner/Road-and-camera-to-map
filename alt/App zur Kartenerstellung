</span><span><span class="hljs-keyword">val</span></span><span> trackedPoints = tracker.points

</span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">startTracking</span></span><span><span class="hljs-params">()</span></span><span> {
    tracker.reset()
    tracker.start()
}

</span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">stopTrackingAndUpload</span></span><span><span class="hljs-params">()</span></span><span> {
    tracker.stop()
    </span><span><span class="hljs-keyword">val</span></span><span> coordinates = tracker.points.value
    </span><span><span class="hljs-keyword">if</span></span><span> (coordinates.size &gt;= </span><span><span class="hljs-number">2</span></span><span>) {
        viewModelScope.launch {
            </span><span><span class="hljs-comment">// lokal speichern</span></span><span>
            </span><span><span class="hljs-keyword">val</span></span><span> id = coordinates.joinToString().hashCode().toString()
            </span><span><span class="hljs-keyword">val</span></span><span> db = MapDatabase.</span><span><span class="hljs-keyword">get</span></span><span>(getApplication())
            db.dao().insertSegment(
                SegmentEntity(
                    id = id,
                    coordinates = coordinates,
                    speedLimit = </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-comment">// optional initial</span></span><span>
                    source = </span><span><span class="hljs-string">"camera"</span></span><span>
                )
            )

            </span><span><span class="hljs-comment">// an Server senden</span></span><span>
            </span><span><span class="hljs-keyword">try</span></span><span> {
                </span><span><span class="hljs-keyword">val</span></span><span> req = SegmentUploadRequest(
                    coordinates = coordinates,
                    speed_limit = </span><span><span class="hljs-number">0</span></span><span>,
                    source = </span><span><span class="hljs-string">"camera"</span></span><span>
                )
                </span><span><span class="hljs-keyword">val</span></span><span> res = ApiClient.api.uploadSegment(req)
                println(</span><span><span class="hljs-string">"Upload erfolgreich: <span class="hljs-subst">${res.status}</span></span></span><span>")
            } </span><span><span class="hljs-keyword">catch</span></span><span> (e: Exception) {
                e.printStackTrace()
            }
        }
    }
}
</span></span></code></div></div></pre>
