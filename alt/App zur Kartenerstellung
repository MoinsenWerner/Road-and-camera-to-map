  .__menu-item-badge:is(:where(.group)[data-disabled] *) { opacity: 0.8; }
  .__menu-item-trailing-btn { align-items: center; align-self: stretch; border-end-end-radius: 10px; border-start-end-radius: 10px; color: var(--text-primary); display: flex; isolation: isolate; margin-block: calc(var(--spacing)*-2); margin-inline-end: calc(var(--spacing)*-2.5); margin-inline-start: calc(var(--spacing)*-1); min-height: calc(var(--spacing)*9); padding-inline-end: calc(var(--spacing)*1.5); padding-inline-start: calc(var(--spacing)*1); pointer-events: auto; position: relative; }
