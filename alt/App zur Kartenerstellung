</span><span><span class="hljs-function"><span class="hljs-keyword">fun</span></span></span><span> </span><span><span class="hljs-title">MainScreen</span></span><span><span class="hljs-params">()</span></span><span> {
    </span><span><span class="hljs-keyword">var</span></span><span> speed </span><span><span class="hljs-keyword">by</span></span><span> remember { mutableStateOf(</span><span><span class="hljs-number">0f</span></span><span>) }

    </span><span><span class="hljs-comment">// Simuliere GPS (wird spÃ¤ter ersetzt)</span></span><span>
    LaunchedEffect(</span><span><span class="hljs-built_in">Unit</span></span><span>) {
        </span><span><span class="hljs-keyword">while</span></span><span> (</span><span><span class="hljs-literal">true</span></span><span>) {
            delay(</span><span><span class="hljs-number">1000</span></span><span>)
            speed = Random.nextFloat() * </span><span><span class="hljs-number">100f</span></span><span>
        }
    }

    Column(
        modifier = Modifier.fillMaxSize().padding(</span><span><span class="hljs-number">16.</span></span><span>dp),
        horizontalAlignment = Alignment.CenterHorizontally,
        verticalArrangement = Arrangement.Center
    ) {
        Text(</span><span><span class="hljs-string">"Live-Geschwindigkeit"</span></span><span>, fontSize = </span><span><span class="hljs-number">22.</span></span><span>sp)
        Text(</span><span><span class="hljs-string">"<span class="hljs-subst">${speed.toInt()}</span></span></span><span> km/h", fontSize = </span><span><span class="hljs-number">40.</span></span><span>sp, style = MaterialTheme.typography.displayMedium)
    }
}
